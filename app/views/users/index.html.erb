<h1>All Users</h1>
<ol id="users">
  <% @users.each do |user| %>
    <li>
    	<%= link_to user.email, user_path(user), class: 'user_link' %>
    	<% if user.admin%>
    	(Admin)
    	<%end%>
    </li>
  <% end %>
</ol>

<%= link_to "Create a New User", new_user_path %>
